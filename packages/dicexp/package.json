  "devDependencies": {
    "@dicexp/build-utils": "workspace:*",
    "@dicexp/builtins": "workspace:*",
    "@dicexp/errors": "workspace:*",
    "@dicexp/lezer": "workspace:*",
    "@dicexp/nodes": "workspace:*",
    "@dicexp/runtime": "workspace:*",
    "@dicexp/test-utils-for-executing": "workspace:*",
    "@lezer/common": "^1.0.3",
    "@types/node": "^20.8.6",
    "@vitest/ui": "^0.34.2",
    "terser": "^5.21.0",
